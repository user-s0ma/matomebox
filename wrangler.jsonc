{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "nectnews",
  "compatibility_date": "2025-04-04",
  "compatibility_flags": ["nodejs_compat"],
  "main": "./workers/app.ts",
  "observability": {
    "enabled": true
  },
  "browser": { "binding": "BROWSER" },
  "workflows": [{ "name": "nectnews-research-workflow", "binding": "RESEARCH_WORKFLOW", "class_name": "ResearchWorkflow" }]
}
